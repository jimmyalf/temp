<project name="Clean Public Project" default="Clean">
	<target name="Clean" description="Cleans deployment directory">
		<echo message="Clearing public deployment folder ${deployPath}" />
		<delete>
			<fileset basedir="${deployPath}">
				<include name="*.config" />
				<include name="*.asax" />
				<include name="bin/**/*.*" />
				<include name="wpc/**/*.*" />
			</fileset>
		</delete>
	</target>
</project>
	