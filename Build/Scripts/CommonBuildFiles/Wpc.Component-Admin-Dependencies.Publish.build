<project name="Deploy Admin Project" default="CopyAdmin">

  <target name="CopyAdmin" description="Copy Admin files">
    <echo message="Copy files for deployment from ${srcAdmin} to ${deployAdmin}" />
    <copy todir="${deployAdmin}" includeemptydirs="true">
      <fileset basedir="${srcAdmin}">
        <include name="bin/*.dll" />
        <exclude name="bin/Spinit.Wpc*.dll" />
      </fileset>
    </copy>
	</target>
  
</project>