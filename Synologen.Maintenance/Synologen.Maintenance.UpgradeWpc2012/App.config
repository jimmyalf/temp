<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<appSettings>
		<!--
		<add key="CommonFilesFolderPath" value="C:\Develop\WPC\Synologen\Common\Files\"/>
		-->
		<add key="CommonFilesFolderPath" value="X:\Synologen\Common\Files"/>
	</appSettings>
	<connectionStrings>
		<!--
		<add name="WpcServer" connectionString="Initial Catalog=dbWpcSynologen;Data Source=.\SQLEXPRESS;uid=sa;pwd=RICE17A;Pooling=true;Connect Timeout=15;" providerName="System.Data.SqlClient"/>
		-->
		<add name="WpcServer" connectionString="Initial Catalog=dbWpcSynologen;Data Source=BLACK;uid=sa;pwd=RICE17A;Pooling=true;Connect Timeout=15;" providerName="System.Data.SqlClient" />
